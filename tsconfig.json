{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/pages/addrest.tsx",
    "src/pages/bookinghistory.tsx",
    "src/pages/login.tsx",
    "src/pages/index.tsx",
    "src/storage/store.ts",
    "src/pages/myrests.tsx",
    "src/pages/subusers.tsx",
    "src/pages/settings.tsx",
    "src/pages/register.tsx",
    "src/pages/pickrest.tsx",
    "src/pages/myrests/[id].tsx",
    "src/components/Forms/TimeTableForm",
    "src/ErrorBoundary.jsx"
  ],
  "exclude": ["node_modules"]
}
